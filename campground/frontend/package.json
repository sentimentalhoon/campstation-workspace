    "date-fns-tz": "^3.2.0",
    "embla-carousel-react": "^8.6.0",
    "exceljs": "^4.4.0",
    "lucide-react": "^0.553.0",
    "next": "16.0.1",
    "qrcode.react": "^4.2.0",
    "react": "19.2.0",
